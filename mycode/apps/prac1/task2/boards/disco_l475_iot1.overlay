/ {

 
    aliases {
        di-led = &di_led;
        ci-led = &ci_led;
    };
    
    leds {
        compatible = "gpio-leds"; 
        di_led: di {
            gpios = <&gpiob 2 GPIO_ACTIVE_HIGH>;   
            label = "DI";
        };

        ci_led: ci {
            gpios = <&gpioa 4 GPIO_ACTIVE_HIGH>;   
            label = "CI";
        };
    };
};
