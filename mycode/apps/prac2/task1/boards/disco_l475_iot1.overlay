
&i2c2 {
    hts221@5f {
        compatible = "st,hts221";
        reg = <0x5f>;
        status = "okay";
    };

    lps22hb-press@5d {
        compatible = "st,lps22hb-press";
        reg = <0x5d>;
        status = "okay";
    };

    lis3mdl-magn@1e {
        compatible = "st,lis3mdl-magn";
        reg = <0x1e>;
        status = "okay";
    };
};


/ {
	aliases {
		rtc = &rtc;
	};
};

&rtc {
    status = "okay";
};


&i2c2 {
    status = "okay";
};
